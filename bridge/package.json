{
  "name": "bridge",
  "private": true,
  "scripts": {
    "start": "cd ./server && npm run start",
    "start:dev": "npm-run-all -p start:client:dev start:server:dev",
    "start:prod": "npm run build && npm run start",
    "start:client:dev": "ng build --watch",
    "start:server:dev": "cd ./server && npm run dev",
    "start:ci": "ng serve --port=3000 --no-live-reload --configuration=test",
    "ng": "ng",
    "build": "ng build --prod --base-href=./",
    "test": "jest --config=jest.config.ts",
    "lint:check": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prettier:check": "prettier --check ./**/*.{js,ts,json,html}",
    "prettier:fix": "prettier --write ./**/*.{js,ts,json,html}",
    "preinstall": "npx npm-force-resolutions",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser=chrome",
    "test:ui": "start-server-and-test start:ci http://localhost:3000 cypress:run"
  },
  "license": "Apache-2.0",
  "author": "Ermin Muratovic <ermin.muratovic@dynatrace.com>",
