{
  "name": "bridge",
  "private": true,
  "scripts": {
    "start": "cd ./server && yarn start",
    "start:dev": "npm-run-all -p start:client:dev start:server:dev",
    "start:prod": "yarn build && yarn start",
    "start:client:dev": "ng build --watch",
    "start:server:dev": "cd ./server && yarn dev",
    "start:ci": "ng serve --port=5000 --no-live-reload --configuration=test",
    "ng": "ng",
    "build": "ng build --prod --base-href=./",
    "test": "jest --config=jest.config.ts --maxWorkers=1",
    "lint:check": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prettier:check": "prettier --check ./**/*.{js,ts,json,html}",
    "prettier:fix": "prettier --write ./**/*.{js,ts,json,html}",
    "cypress:open": "TZ=Europe/Berlin start-server-and-test 'yarn start:ci' http://localhost:5000 'cypress open'",
    "cypress:open:win32": "set TZ=Europe/Berlin && start-server-and-test 'yarn start:ci' http://localhost:5000 'cypress open'",
    "cypress:run": "TZ=Europe/Berlin cypress run --browser=chrome",
    "cypress:run:win32": "set TZ=Europe/Berlin && yarn cypress run --browser=chrome",
    "test:ui": "start-server-and-test 'yarn start:ci' http://localhost:5000 'yarn cypress:run'",
    "test:ui:win32": "start-server-and-test 'yarn start:ci' http://localhost:5000 'yarn cypress:run:win32'"
  },
  "license": "Apache-2.0",
  "author": "Ermin Muratovic <ermin.muratovic@dynatrace.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keptn/keptn.git"
  },
  "bugs": {
    "url": "https://github.com/keptn/keptn/issues"
  },
  "keywords": [
    "keptn",
    "monitoring",
    "logs"
  ],
