{
  "name": "bridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server/bin/www",
    "start:dev": "npm-run-all -p start:server:dev start:client:dev",
    "start:server:dev": "nodemon server/bin/www --watch server/",
    "start:client:dev": "ng build --watch",
    "ng": "ng",
    "server": "node server/bin/www",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "connect:all": "npm-run-all -p connect-keptn-bridge connect-datastore connect-configuration-service",
    "connect-keptn-bridge": "kubectl port-forward -n keptn svc/bridge 9000:8080",
    "connect-datastore": "kubectl port-forward -n keptn-datastore svc/mongodb-datastore 8085:8080",
    "connect-configuration-service": "kubectl port-forward -n keptn svc/configuration-service 8086:8080"
  },
  "license": "Apache-2.0",
  "author": "Ermin Muratovic <ermin.muratovic@dynatrace.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keptn/bridge.git"
  },
  "bugs": {
    "url": "https://github.com/keptn/bridge/issues"
  },
  "keywords": [
    "keptn",
    "monitoring",
    "logs"
  ],
  "dependencies": {
    "@angular/animations": "^8.0.0",
